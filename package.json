{
  "name": "fediship",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "setup": "bundle install",
    "ios:install": "bundle exec pod repo update && cd ios && bundle exec pod install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:stories": "APP_ENV=stories react-native start",
    "bundle": "mkdir -p build && react-native bundle --bundle-output build/bundle --sourcemap-output build/bundle.map --dev false --entry-file index.js",
    "sourcemap-explorer": "source-map-explorer build/bundle build/bundle.map --json build/explorer.json",
    "oss-license": "mkdir -p build && react-native-oss-license --json > build/licenses.json",
    "build-licenses": "mkdir -p src/generated && echo \"[]\" > src/generated/licenses.json && yarn oss-license && yarn bundle && yarn sourcemap-explorer && ts-node scripts/licenses.ts > src/generated/licenses.json",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "types": "tsc --noEmit",
    "go": "code . && open -a Simulator && yarn start",
    "ci": "yarn lint && yarn types",
    "gather-licenses": "license-report --only=prod --output=json --config=scripts/license-report-config.json > scripts/out/licenses.json",
    "postinstall": "patch-package",
    "flightcheck": "yarn lint --cache && yarn types && yarn test"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.3.2",
    "@legendapp/state": "^1.0.0-rc.2",
    "@openspacelabs/react-native-zoomable-view": "^2.0.4",
    "@react-native-community/netinfo": "^9.0.0",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@sterlingwes/microformats-parser": "^1.4.2",
    "date-fns": "^2.28.0",
    "nanoid": "^3.3.4",
    "react": "18.2.0",
    "react-native": "0.73.1",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-image-picker": "^4.9.0",
    "react-native-mmkv": "^2.5.1",
    "react-native-reanimated": "^3.0.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^12.3.0",
    "react-native-swipe-list": "https://github.com/sterlingwes/react-native-swipe-list",
    "react-native-toast-message": "^2.1.5",
    "react-native-video": "^5.2.0",
    "react-native-video-controls": "^2.8.1",
    "react-native-webview": "^13.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-syntax-typescript": "^7.23.3",
    "@babel/plugin-transform-arrow-functions": "^7.23.3",
    "@babel/plugin-transform-optional-chaining": "^7.23.4",
    "@babel/plugin-transform-shorthand-properties": "^7.23.3",
    "@babel/plugin-transform-template-literals": "^7.23.3",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/react-native": "^9.1.0",
    "@types/jest": "^26.0.23",
    "@types/parse5": "^6.0.3",
    "@types/react": "^18.2.6",
    "@types/react-native": "^0.70.14",
    "@types/react-native-htmlview": "^0.16.0",
    "@types/react-native-video": "^5.0.13",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-global-define": "^1.0.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "license-report": "^5.0.2",
    "mockdate": "^3.0.5",
    "patch-package": "^6.4.7",
    "prettier": "^2.8.8",
    "react-native-oss-license": "^0.6.0",
    "react-native-svg-transformer": "^1.2.0",
    "react-test-renderer": "18.2.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^10.8.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "preset": "react-native",
    "globals": {
      "__TEST__": true
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
